BRANCH=$(echo "$CIRCLE_BRANCH" | awk '{print tolower($0)}')
VERSION=$(gitversion /showvariable SemVer)
if [ "$BRANCH" == "master" ] | [ "$BRANCH" == "develop" ]; then
    VERSION=$(gitversion /showvariable MajorMinorPatch)
else
   FullSemVer=$(gitversion /showvariable FullSemVer)
   VERSION="${FullSemVer}"
fi
COMMAND=$@
COMMAND="${COMMAND/x.x.x/${VERSION}}"
exec $COMMAND